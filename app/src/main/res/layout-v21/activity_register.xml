<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#E3F8F1"
    android:layout_height="match_parent"
    tools:context=".register">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/sonalabel1"/>
        <com.tomer.fadingtextview.FadingTextView
            android:id="@+id/fadingtext1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:background="#ffffff"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold"
            app:texts="@array/example"
            app:timeout="1000" />




        <TextView
            android:layout_marginLeft="20sp"
            android:layout_marginTop="30sp"
            android:layout_width="120dp"
            android:layout_height="31dp"
            android:text="Name"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold"
            android:gravity="start" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="31dp"

            android:layout_marginLeft="20sp"
            android:layout_marginTop="30sp"
            android:gravity="start"
            android:text="Gender"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="120dp"
            android:layout_height="31dp"

            android:layout_marginLeft="20sp"
            android:layout_marginTop="30sp"
            android:gravity="start"
            android:text="Email"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="120dp"
            android:layout_height="31dp"

            android:layout_marginLeft="20sp"
            android:layout_marginTop="30sp"
            android:gravity="start"
            android:text="Mobile"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold" />
        <TextView
            android:layout_marginLeft="20sp"
            android:layout_marginTop="30sp"

            android:layout_width="120dp"
            android:layout_height="31dp"
            android:text="Branch"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold"
            android:gravity="start" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="31dp"

            android:layout_marginLeft="20sp"
            android:layout_marginTop="30sp"
            android:gravity="start"
            android:text="Department"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="31dp"

            android:layout_marginLeft="20sp"
            android:layout_marginTop="30sp"
            android:gravity="start"
            android:text="Year of study"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="120dp"
            android:layout_height="31dp"

            android:layout_marginLeft="20sp"
            android:layout_marginTop="30sp"
            android:gravity="start"
            android:text="College Name"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="120dp"
            android:layout_height="31dp"

            android:layout_marginLeft="20sp"
            android:layout_marginTop="30sp"
            android:gravity="start"
            android:text="Workshops"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="120dp"
            android:layout_height="31dp"

            android:layout_marginLeft="20sp"
            android:layout_marginTop="120sp"
            android:gravity="start"
            android:text="Events List"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold" />





    </LinearLayout>
    <LinearLayout
        android:layout_width="10sp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="194sp"
        android:layout_marginLeft="140dp">
        <com.tomer.fadingtextview.FadingTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textAlignment="textStart"
            android:gravity="start"
            android:textStyle="bold"
            android:textColor="#000000"
            app:timeout="100"
            app:texts="@array/blinker"/>
        <com.tomer.fadingtextview.FadingTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textAlignment="textStart"
            android:gravity="start"
            android:textStyle="bold"
            android:textColor="#000000"
            app:texts="@array/blinker"
            app:timeout="100"
            android:layout_marginTop="40sp"/>
        <com.tomer.fadingtextview.FadingTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textAlignment="textStart"
            android:gravity="start"
            android:textStyle="bold"
            android:textColor="#000000"
            app:texts="@array/blinker"
            app:timeout="100"
            android:layout_marginTop="40sp"/>
        <com.tomer.fadingtextview.FadingTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textAlignment="textStart"
            android:gravity="start"
            android:textStyle="bold"
            android:textColor="#000000"
            app:texts="@array/blinker"
            app:timeout="100"
            android:layout_marginTop="40sp"/>
        <com.tomer.fadingtextview.FadingTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textAlignment="textStart"
            android:gravity="start"
            android:textStyle="bold"
            android:textColor="#000000"
            app:texts="@array/blinker"
            app:timeout="100"
            android:layout_marginTop="42sp"/>
        <com.tomer.fadingtextview.FadingTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textAlignment="textStart"
            android:gravity="start"
            android:textStyle="bold"
            android:textColor="#000000"
            app:texts="@array/blinker"
            app:timeout="100"
            android:layout_marginTop="41sp"/>
        <com.tomer.fadingtextview.FadingTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textAlignment="textStart"
            android:gravity="start"
            android:textStyle="bold"
            android:textColor="#000000"
            app:texts="@array/blinker"
            app:timeout="100"
            android:layout_marginTop="41sp"/>
        <com.tomer.fadingtextview.FadingTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textAlignment="textStart"
            android:gravity="start"
            android:textStyle="bold"
            android:textColor="#000000"
            app:texts="@array/blinker"
            app:timeout="100"
            android:layout_marginTop="40sp"/>


    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="194sp"
        android:layout_marginLeft="160dp">
        <EditText
            android:id="@+id/rname"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:inputType="text"
            android:cursorVisible="true"
            android:outlineProvider="none"
            android:background="#E3F8F1"


            />
        <EditText
            android:id="@+id/rgender"
            android:layout_marginTop="40dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="#000000"
            android:hint="Male or Female"
            android:inputType="text"
            android:cursorVisible="true"
            android:outlineProvider="none"
            android:background="#E3F8F1"


            />
        <EditText
            android:id="@+id/remail"
            android:layout_marginTop="40dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="#000000"
            android:inputType="textEmailAddress"
            android:cursorVisible="true"
            android:outlineProvider="none"
            android:background="#E3F8F1"


            />
        <EditText
            android:id="@+id/rmobile"
            android:layout_marginTop="40dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:hint="Eg:-9876543210"
            android:inputType="phone"
            android:cursorVisible="true"
            android:outlineProvider="none"
            android:background="#E3F8F1"


            />
        <EditText
            android:id="@+id/rbranch"
            android:layout_marginTop="45dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:inputType="text"
            android:cursorVisible="true"
            android:outlineProvider="none"
            android:background="#E3F8F1"


            />
        <EditText
            android:id="@+id/rdepartment"
            android:layout_marginTop="40dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:inputType="text"
            android:cursorVisible="true"
            android:outlineProvider="none"
            android:background="#E3F8F1"


            />
        <EditText
            android:id="@+id/ryear"
            android:layout_marginTop="40dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:inputType="number"
            android:cursorVisible="true"
            android:outlineProvider="none"
            android:background="#E3F8F1"


            />
        <EditText
            android:id="@+id/rcollege"
            android:layout_marginTop="40dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:inputType="text"
            android:cursorVisible="true"
            android:outlineProvider="none"
            android:background="#E3F8F1"


            />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="700dp"
        android:layout_marginLeft="100dp">
        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/radiogroup">


        <RadioButton
            android:id="@+id/rmachine"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Machine learning using python"
            android:textStyle="normal"
            android:textSize="15sp"
            android:textColor="#000000"/>
        <RadioButton
            android:id="@+id/randroid"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Augmented reality in Android"
            android:textStyle="normal"
            android:textSize="15sp"
            android:textColor="#000000"/>
        </RadioGroup>
        <CheckBox
            android:id="@+id/rignite"
            android:layout_marginTop="80dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="IGNITE"
            android:textStyle="normal"
            android:textSize="15sp"
            android:textColor="#000000"/>

        <CheckBox
            android:id="@+id/rspot"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SPOT ERROR"
            android:textStyle="normal"
            android:textSize="15sp"
            android:textColor="#000000"/>
        <CheckBox
            android:id="@+id/rblind"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="BLIND CODING"
            android:textStyle="normal"
            android:textSize="15sp"
            android:textColor="#000000"/>
        <CheckBox
            android:id="@+id/rkahoot"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="KAHOOT"
            android:textStyle="normal"
            android:textSize="15sp"
            android:textColor="#000000"/>
        <CheckBox
            android:id="@+id/rphotography"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="PHOTOGRAPHY"
            android:textStyle="normal"
            android:textSize="15sp"
            android:textColor="#000000"/>
        <CheckBox
            android:id="@+id/rhunt"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="HUNT EM UP"
            android:textStyle="normal"
            android:textSize="15sp"
            android:textColor="#000000"/>
        <CheckBox
            android:id="@+id/rsql"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SQL QUERY"
            android:textStyle="normal"
            android:textSize="15sp"
            android:textColor="#000000"/>
        <CheckBox
            android:id="@+id/rpaper"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="PAPER PRESENTATION"
            android:textStyle="normal"
            android:textSize="15sp"
            android:textColor="#000000"/>
        <Button
            android:id="@+id/rbutton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SUBMIT"
            android:textColor="#000000"
            android:textStyle="bold"
            android:textSize="20sp"
            android:outlineProvider="none"
            android:background="#E3F8F1"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="50sp"
            android:textAlignment="textStart"
            android:layout_marginBottom="50dp"/>

    </LinearLayout>
        </RelativeLayout>


    </ScrollView>



</RelativeLayout>